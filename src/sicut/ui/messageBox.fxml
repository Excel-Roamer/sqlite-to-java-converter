<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="200.0" prefWidth="400.0" style="-fx-border-color: grey; -fx-border-radius: 2px; -fx-border-style: solid; -fx-border-width: 1px;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sicut.ui.MessageBoxController">
   <left>
      <Label fx:id="msgIcon" alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" minWidth="-Infinity" prefWidth="96.0" text="" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
   </left>
   <center>
      <Label fx:id="msg" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="" BorderPane.alignment="CENTER">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Label>
   </center>
   <bottom>
      <AnchorPane maxWidth="1.7976931348623157E308" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="cancel" minHeight="30.0" minWidth="64.0" mnemonicParsing="false" text="%cancel" visible="false" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
            <Button fx:id="ok" minHeight="30.0" minWidth="64.0" mnemonicParsing="false" text="%ok" visible="true" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
            <Button fx:id="yes" minHeight="30.0" minWidth="64.0" mnemonicParsing="false" text="%yes" visible="false" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
            <Button fx:id="no" minHeight="30.0" minWidth="64.0" mnemonicParsing="false" text="%no" visible="false" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
         </children>
      </AnchorPane>
   </bottom>
   <top>
      <StackPane BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="msgTitle" maxWidth="1.7976931348623157E308" prefHeight="48.0" text="">
               <padding>
                  <Insets left="20.0" right="20.0" />
               </padding>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <FlowPane alignment="TOP_RIGHT">
               <children>
                  <Button fx:id="close" mnemonicParsing="false" style="-fx-background-color: rgba(0,10,19,0.1);" text="x" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </FlowPane>
         </children>
      </StackPane>
   </top>
</BorderPane>
